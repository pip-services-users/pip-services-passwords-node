{
    "name": "pip-services-passwords-node",
    "version": "1.1.0",
    "author": "Conceptual Vision Consulting LLC",
    "description": "Password authentication microservice in Node.js",
    "contributors": [
        {
            "name": "Sergey Seroukhov",
            "email": "seroukhov@gmail.com"
        }
    ],
    "main": "./obj/src/index.js",
    "typings": "./obj/src/index.d.ts",
    "keywords": [
        "pip.services",
        "users",
        "password",
        "authentication"
    ],
    "noAnalyze": true,
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:pip-services-users/pip-services-passwords-node.git"
    },
    "license": "MIT",
    "engines": {
        "node": ">=0.6.14"
    },
    "private": false,
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "protogen": "cd src/protos && protoc --plugin=protoc-gen-ts=../../node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:./ --ts_out=. --grpc_out=. --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` passwords_v1.proto",
        "launch": "node run",
        "relaunch": "tsc && node run",
        "test": "mocha -t 20000 -R spec -u tdd --recursive ./obj/test",
        "retest": "tsc && mocha -t 20000 -R spec -u tdd --recursive ./obj/test",
        "benchmark": "matcha -R clean -I tdd ./obj/benchmark/run.js",
        "rebenchmark": "tsc && matcha -R clean -I tdd ./obj/benchmark/run.js"
    },
    "dependencies": {
        "pip-services3-commons-node": "^3.0.0",
        "pip-services3-components-node": "^3.0.0",
        "pip-services3-container-node": "^3.0.0",
        "pip-services3-couchbase-node": "^3.0.0",
        "pip-services3-data-node": "^3.0.0",
        "pip-services3-aws-node": "^3.0.0",
        "pip-services3-rpc-node": "^3.0.0",
        "pip-services3-grpc-node": "^3.0.0",
        "pip-services3-mongodb-node": "^3.0.0",
        "pip-clients-activities-node": "^1.0.1",
        "pip-clients-msgdistribution-node": "^1.0.1"
    },
    "devDependencies": {
        "google-protobuf": "^3.7.1",
        "ts-protoc-gen": "^0.9.0",
        "@types/node": "*",
        "@types/lodash": "^4.0.0",
        "@types/async": "^2.0.0",
        "@types/chai": "*",
        "@types/mocha": "*",
        "@types/mongodb": "^2.1.38",
        "restify": "^4.3.0",
        "chai": "*",
        "mocha": "*"
    }
}
